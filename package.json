{
  "name": "poss",
  "version": "3.0.1",
  "description": "Cleaner try-catch. 'This poss might work'",
  "keywords": [
    "better",
    "looking",
    "try",
    "catch"
  ],
  "author": "Matthew Mueller <mattmuelle@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git://github.com/MatthewMueller/poss.git"
  },
  "dependencies": {},
  "devDependencies": {
    "async-to-gen": "1.1.3",
    "babel-eslint": "6.1.2",
    "co-mocha": "1.1.3",
    "devtool": "2.2.0",
    "mocha": "3.0.2",
    "nyc": "8.3.0",
    "snazzy": "5.0.0"
  },
  "scripts": {
    "test": "nyc mocha",
    "test:browser": "devtool node_modules/mocha/bin/_mocha -qc -- test/",
    "test:watch": "mocha -w",
    "test:cov": "nyc --reporter=lcov mocha && open coverage/lcov-report/index.html",
    "test:only": "mocha",
    "lint": "snazzy",
    "pretest": "npm run lint"
  },
  "main": "index",
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "it",
      "before",
      "beforeEach",
      "after",
      "afterEach"
    ]
  }
}